    "minimapSlider.activeBackground": "#1f1f1f80"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a8e6cf",
        "fontStyle": ""
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#b4f4e0",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#7dd3c0"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a8d5e2"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#a8e6cf",
        "fontStyle": ""
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "variable.language"
      ],
      "settings": {
        "foreground": "#b4f4e0",
        "fontStyle": ""
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.object.property",
        "support.variable.property",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#a8e6cf",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#a8e6cf"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b4f4e0"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f9e79f",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#a8d5e2"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#a8e6cf"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#a8e6cf",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e5e5e5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#e5e5e5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#7dd3c0",
        "background": "#141414"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#a8d5e2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#f9e79f"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#0a0a0a",
        "background": "#f9e79f"
      }
    }
  ]
}
