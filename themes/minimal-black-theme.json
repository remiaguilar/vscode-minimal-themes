    "statusBar.debuggingForeground": "#0a0a0a",

    "titleBar.activeBackground": "#0a0a0a",
    "titleBar.activeForeground": "#e5e5e5",
    "titleBar.inactiveBackground": "#0a0a0a",
    "titleBar.inactiveForeground": "#999999",
    "titleBar.border": "#1f1f1f",

    "editorGroupHeader.tabsBackground": "#0a0a0a",
    "editorGroupHeader.tabsBorder": "#1f1f1f",
    "tab.activeBackground": "#141414",
    "tab.activeForeground": "#e5e5e5",
    "tab.activeBorder": "#00000000",
    "tab.activeBorderTop": "#a8e6cf",
    "tab.inactiveBackground": "#0a0a0a",
    "tab.inactiveForeground": "#999999",
    "tab.border": "#1f1f1f",
    "tab.hoverBackground": "#141414",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.unfocusedActiveBorderTop": "#a8e6cf80",

    "breadcrumb.background": "#0a0a0a",
    "breadcrumb.foreground": "#999999",
    "breadcrumb.focusForeground": "#e5e5e5",
    "breadcrumb.activeSelectionForeground": "#a8e6cf",
    "breadcrumbPicker.background": "#141414",

    "list.activeSelectionBackground": "#1f1f1f",
    "list.activeSelectionForeground": "#e5e5e5",
    "list.inactiveSelectionBackground": "#141414",
    "list.inactiveSelectionForeground": "#e5e5e5",
    "list.hoverBackground": "#141414",
    "list.hoverForeground": "#e5e5e5",
